---
import { VariantTabs } from "@/components/VariantTabs";
const { solidDisabled, vueDisabled } = Astro.props;
---

<VariantTabs
  client:only="react"
  solidDisabled={solidDisabled}
  vueDisabled={vueDisabled}
>
  <Fragment slot="reactTab">
    <slot name="react" />
  </Fragment>
  <Fragment slot="solidTab">
    <slot name="solid" />
  </Fragment>
  <Fragment slot="vueTab">
    <slot name="vue" />
  </Fragment>
</VariantTabs>
