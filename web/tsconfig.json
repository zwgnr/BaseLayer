{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@ui/*": ["src/ui/*"],
      "@lib/*": ["src/lib/*"],
      "@potions/*": ["src/potions/*"]
    }
  },
}
