{
  "name": "potionui",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ComponentParser": "ts-node --esm utils/ComponentParser.ts",
    "PotionParser": "ts-node --esm utils/PotionParser.ts",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "lint-staged": {
    "web/src/potions/*": [
      "pnpm run PotionParser"
    ],
    "web/src/ui/**/*": [
      "pnpm run ComponentParser",
      "pnpm dlx prettier --write './web/src/json/components.ts'"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-solid": "^0.12.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2"
  }
}
