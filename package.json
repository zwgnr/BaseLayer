    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-solid": "^0.12.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2"
  }
